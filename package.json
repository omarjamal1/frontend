{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@amcharts/amcharts3-react": "^3.0.0",
    "@amcharts/amcharts4": "^4.0.1",
    "@progress/kendo-react-dateinputs": "^2.3.1",
    "@progress/kendo-react-dialogs": "^2.3.1",
    "@progress/kendo-react-dropdowns": "^2.3.1",
    "@progress/kendo-react-inputs": "^2.3.1",
    "@progress/kendo-react-intl": "^2.3.1",
    "@progress/kendo-react-popup": "^2.3.1",
    "@progress/kendo-theme-material": "^1.4.0",
    "amcharts3": "^3.21.14",
    "material-design-lite": "^1.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "2.1.1",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/3.*",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build:clean",
    "build:clean": "cd build && mv static/js/main.*.js ../static/frontend/main.js && mv static/css/main.*.css ../static/frontend/main.css && mv static/js/3.*.js ../static/frontend/external.js && mv static/css/3.*.css ../static/frontend/external.css && mv index.html ../templates/frontend/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "rsync -avzhe ssh --progress ./build/* /www/agview/frontend"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "http://dev.agviewer.net/frontend"
}
